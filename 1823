class Solution(object):
    def findTheWinner(self, n, k):
        """
        :type n: int
        :type k: int
        :rtype: int
        """
        q = deque(range(1, n + 1)) 
        
        while len(q) > 1:
          
            
            for i in range(0,k - 1):
                q.append(q.popleft())
         
            q.popleft()
        
        return q[0]
        
        
